<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ScaleLayout">
        <attr name="scale" format="float"/>
    </declare-styleable>
    <declare-styleable name="ExpandableTextView">
        <attr name="maxCollapsedLines" format="integer"/>
        <attr name="animDuration" format="integer"/>
        <attr name="animAlphaStart" format="float"/>
        <attr name="expandDrawable" format="reference"/>
        <attr name="collapseDrawable" format="reference"/>

        <attr name="collapse_text" format="string" />
        <attr name="fold_text" format="string" />
        <attr name="max_lines" format="integer" />
        <attr name="additional_text_color" format="color" />
        <attr name="underlined" format="boolean" />
    </declare-styleable>
</resources>